@model List<MovieReviewer.Models.Movie>

<h1>Movie</h1>
<hr />
<a asp-action="Create" asp-controller="Movie">Add Movie to List</a>
<ul>
    @foreach (var movie in Model)
    {
        <li>@movie.Name (@movie.Rating) <a asp-action="Create" asp-controller="Review" asp-route-game="@movie.Name">Review movie</a></li>
    }
</ul>